                            <section id="faq">
                                <div class="container mx-auto px-8 flex flex-col max-w-5xl mt-10 md:mt-14 py-20" id="faq">
                                    <h2 class="text-6xl md:text-6xl lg:text-7xl font-display leading-none text-center font-bold text-white relative mb-8 md:mb-12">FAQ</h2>
                                    <dl class="mt-6 space-y-8 divide-y divide-gray-200/20">
